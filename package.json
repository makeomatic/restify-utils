{
  "name": "restify-utils",
  "version": "0.1.9",
  "description": "Convenient utils for handling dynamic routing and configuration with restify framework",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "if [[ $NODE_ENV != \"production\" ]]; then ./node_modules/.bin/babel -d ./lib --optional es7.objectRestSpread,es7.classProperties,es7.decorators ./src; fi",
