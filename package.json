  "bugs": {
    "url": "https://github.com/makeomatic/restify-utils/issues"
  },
  "homepage": "https://github.com/makeomatic/restify-utils#readme",
  "dependencies": {
    "common-errors": "^0.5.3",
    "debug": "^2.2.0",
    "glob": "^6.0.1",
    "lodash": "^3.10.1",
    "ms-amqp-validation": "^0.4.1"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.33",
    "babel-eslint": "^4.1.4",
    "chai": "^3.4.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-mocha": "^1.0.0",
    "eslint-plugin-react": "^3.8.0",
    "mocha": "^2.3.3"
