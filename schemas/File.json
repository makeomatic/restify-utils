{
  "id": "File",
  "type": "object",
  "additionalProperties": false,
  "required": [ "id", "attributes" ],
  "properties": {
    "type": {
      "type": "string",
      "enum": [ "file" ]
    },
    "id": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-4[0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$"
    },
    "attributes": {
      "type": "object",
      "properties": {
        "contentType": {
          "type": "string"
        },
        "contentLength": {
          "type": "integer"
        },
        "md5Hash": {
          "type": "string",
          "length": 32
        },
        "owner": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "enum": [ "pending", "uploaded", "processed" ]
        },
        "startedAt": {
          "type": "integer"
        },
        "humanName": {
          "type": "string"
        }
      }
    }
  }
}